<?xml version="1.0"?>
<Container version="2">
  <Name>Valheim Server</Name>
  <Repository>lloesche/valheim-server</Repository>
  <Registry>https://hub.docker.com/r/lloesche/valheim-server/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/lloesche/valheim-server/</Support>
  <Project/>
  <Overview>Self-hosted Valheim Server Docker Container.</Overview>
  <Category>Network:Management Tools:</Category>
  <WebUI>http://[IP]:[PORT:9001]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/ich777/docker-templates/master/ich777/images/valheim.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <DateInstalled>1527398179</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <DonateImg/>
  <MinVer/>
  <Description>Self-hosted Valheim Server Docker Container.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish/>
  </Networking>
  <Labels/>
  <Config Name="Server Name" Target="SERVER_NAME" Default="SSoG-Valiheim" Mode="" Description="Server Name. (No spaces allowed)" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="World Name" Target="WORLD_NAME" Default="I Love Llamas" Mode="" Description="Change to create new worlds...?" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="Public Server" Target="SERVER_PUBLIC" Default="" Mode="" Description="Register Server in Valheim listings." Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="Server Password" Target="SERVER_PASS" Default="" Mode="" Description="Password for players to access the Valheim server. (5 characters or greater)" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="Status Page" Target="STATUS_HTTP" Default="false" Mode="" Description="Change to create new worlds...?" Type="Variable" Display="always" Required="false" Mask="true"/> 
  <Config Name="Supervisor Page" Target="SUPERVISOR_HTTP" Default="false" Mode="" Description="Change to create new worlds...?" Type="Variable" Display="always" Required="false" Mask="true"/>
  <Config Name="Config" Target="/config" Default="/mnt/user/appdata/valheim/config" Mode="rw" Description="Path where shared folders are located.&amp;lt;br&amp;gt;Container Path: /config" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Data" Target="/opt/valheim" Default="/mnt/user/appdata/valheim/data" Mode="rw" Description="Path where shared folders are located.&amp;lt;br&amp;gt;Container Path: /opt/valheim" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Game Port 1" Target="2456" Default="2456" Mode="udp" Description="Container Port: 2456" Type="Port" Display="always" Required="true" Mask="false">2456</Config>
  <Config Name="Game Port 2" Target="2457" Default="2457" Mode="udp" Description="Container Port: 2457" Type="Port" Display="always" Required="true" Mask="false">2457</Config>
  <Config Name="Status Port" Target="80" Default="80" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false">80</Config>
  <Config Name="Supervisor Port" Target="9001" Default="9001" Mode="tcp" Description="Container Port: 9001" Type="Port" Display="always" Required="true" Mask="false">9001</Config>

</Container>